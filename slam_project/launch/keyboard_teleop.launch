<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <!-- Teleop Launch -->
  <node pkg="turtlebot_teleop" type="turtlebot_teleop_key" name="slambot_teleop_keyboard" output="screen">
    <param name="scale_linear" value="0.5" type="double"/>
    <param name="scale_angular" value="0.2" type="double"/>
    <remap from="slambot_teleop_keyboard/cmd_vel" to="cmd_vel"/>
  </node>

</launch>